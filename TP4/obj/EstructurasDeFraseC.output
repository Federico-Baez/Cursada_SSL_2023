Gramática

    0 $accept: input $end

    1 input: /* vacío */
    2      | input line

    3 line: '\n'
    4     | sentencia
    5     | sentencia '\n'
    6     | error
    7     | error ';'

    8 expresion: expresionAsignacion

    9 expresionAsignacion: expresionCondicional
   10                    | expresionUnaria operacionAsignacion expresionAsignacion

   11 operacionAsignacion: '='
   12                    | SUMADIRECTA
   13                    | RESTADIRECTA

   14 expresionCondicional: expresionOr
   15                     | expresionOr '?' expresion ':' expresion

   16 expresionOr: expresionAnd
   17            | expresionOr OR expresionAnd

   18 expresionAnd: expresionIgualdad
   19             | expresionAnd AND expresionIgualdad

   20 expresionIgualdad: expresionRelacional
   21                  | expresionIgualdad operadorComparacion expresionRelacional

   22 operadorComparacion: IGUALDAD DESIGUALDAD

   23 expresionRelacional: expresionAditiva
   24                    | expresionRelacional operadorRelacional expresionAditiva

   25 operadorRelacional: '>'
   26                   | '<'
   27                   | MENORIGUAL
   28                   | MAYORIGUAL

   29 expresionAditiva: expresionMultiplicativa
   30                 | expresionAditiva '+' expresionMultiplicativa
   31                 | expresionAditiva '-' expresionMultiplicativa

   32 expresionMultiplicativa: expresionUnaria
   33                        | expresionMultiplicativa '*' expresionUnaria
   34                        | expresionMultiplicativa '/' expresionUnaria

   35 expresionUnaria: expresionPostfijo
   36                | expresionUnaria INCREMENTO
   37                | expresionUnaria DECREMENTO
   38                | operadorUnario expresionUnaria
   39                | '-' expresionUnaria
   40                | '+' expresionUnaria
   41                | '*' expresionUnaria

   42 operadorUnario: '&'
   43               | '!'

   44 expresionPostfijo: expresionPrimaria
   45                  | expresionPostfijo '[' expresion ']'
   46                  | expresionPostfijo '(' listaDeArgumentos ')'

   47 listaDeArgumentos: argumento
   48                  | listaDeArgumentos ',' argumento

   49 argumento: expresion

   50 expresionPrimaria: CONSTANTES
   51                  | IDENTIFICADOR
   52                  | LITERAL_CADENA

   53 declaracion: declaracionVariable ';'
   54            | declaracionAsignacion ';'
   55            | declaracionFuncion ';'

   56 declaracionFuncion: TIPOS_DE_DATOS IDENTIFICADOR '(' listaDeParametros ')'
   57                   | VOID IDENTIFICADOR '(' listaDeParametros ')'

   58 definicionFuncion: TIPOS_DE_DATOS IDENTIFICADOR '(' listaDeParametros ')' sentenciaCompuesta
   59                  | VOID IDENTIFICADOR '(' listaDeParametros ')' sentenciaCompuesta

   60 listaDeParametros: /* vacío */
   61                  | parametro
   62                  | listaDeParametros ',' parametro

   63 parametro: TIPOS_DE_DATOS IDENTIFICADOR

   64 declaracionVariable: TIPOS_DE_DATOS listaDeVariables

   65 declaracionAsignacion: TIPOS_DE_DATOS listaDeVariablesAsignacion

   66 listaDeVariables: variable
   67                 | variable ',' listaDeVariables

   68 listaDeVariablesAsignacion: variableAsignacion
   69                           | variableAsignacion ',' listaDeVariablesAsignacion

   70 variable: IDENTIFICADOR

   71 variableAsignacion: IDENTIFICADOR '=' expresionAsignacion

   72 sentenciaCompuesta: '{' listaCompuesta '}'

   73 listaCompuesta: /* vacío */
   74               | line listaCompuesta

   75 sentencia: sentenciaCompuesta
   76          | sentenciaExpresion
   77          | sentenciaSeleccion
   78          | sentenciaIteracion
   79          | sentenciaSalto
   80          | declaracion
   81          | definicionFuncion

   82 sentenciaExpresion: ';'
   83                   | expresion ';'

   84 sentenciaSeleccion: IF '(' expresion ')' sentencia ELSE sentencia
   85                   | IF '(' expresion ')' sentencia

   86 sentenciaIteracion: WHILE '(' expresion ')' sentencia
   87                   | DO sentencia WHILE '(' expresion ')' ';'
   88                   | FOR '(' expresionOP ';' expresionOP ';' expresionOP ')' sentenciaCompuesta

   89 sentenciaSalto: RETURN expresion ';'

   90 expresionOP: /* vacío */
   91            | expresion


